<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Booking Test Suite">
    <test name="Test Authorization">
        <classes>
            <class name="pl.mradomyski.restassured.tests.AuthorizationTest"/>
        </classes>
    </test>

    <test name="Test Getting Booking Ids">
        <classes>
            <class name="pl.mradomyski.restassured.tests.GetBookingIdsTest"/>
        </classes>
    </test>

    <test name="Test Getting Specific Booking">
        <classes>
            <class name="pl.mradomyski.restassured.tests.GetBookingTest"/>
        </classes>
    </test>

    <test name="Test Update Specific Booking With Random Data">
        <classes>
            <class name="pl.mradomyski.restassured.tests.UpdateBookingTest"/>
        </classes>
    </test>
</suite>